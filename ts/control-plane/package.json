{
  "name": "control-plane",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack ${OUT_DIR:+ -o ${OUT_DIR}}",
    "build:server": "rimraf ${OUT_DIR:-dist} && tsc --outDir ${OUT_DIR:-dist} && tsc-alias --dir ${OUT_DIR:-dist}",
    "build:server:watch": "rimraf ${OUT_DIR:-dist} && tsc --outDir ${OUT_DIR:-dist} && (concurrently \"tsc -w --outDir ${OUT_DIR:-dist}\" \"tsc-alias -w --dir ${OUT_DIR:-dist}\")",
    "start:server": "ts-node src/server/run_server.ts",
    "start:server:debug:ts": "ts-node-dev --respawn --inspect -r tsconfig-paths/register -- src/server/run_server.ts",
    "start:server:debug": "webpack --watch --mode development",
    "start:client": "nodemon src/client/index.ts --watch src/client",
    "proto:build": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useOptionals=messages,exportCommonSymbols=true,esModuleInterop=true,outputTypeRegistry=true,useExactTypes=false --ts_proto_out=./src/proto/ -I ../../protos/ ../../protos/mrc/protos/architect.proto",
    "proto:build2": "grpc_tools_node_protoc --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin --ts_proto_opt=outputServices=grpc-js,useOptionals=messages,exportCommonSymbols=false,esModuleInterop=true --ts_proto_out=./src/proto/ -I ../../protos/ ../../protos/mrc/protos/architect.proto",
    "proto:build3": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/proto --grpc_out=grpc_js:./src/proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/proto -I ../../protos/ ../../protos/mrc/protos/architect.proto",
    "codegen:buf": "cd proto; buf generate; cd ..",
    "test": "jest",
    "start:devtools": "redux-devtools"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@redux-devtools/cli": "^2.0.0",
    "@redux-devtools/remote": "^0.8.0",
    "@swc/jest": "^0.2.24",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "nodemon": "^2.0.20",
    "nodemon-webpack-plugin": "^4.8.1",
    "require-json5": "^1.3.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^1.139.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.5",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.7",
    "@reduxjs/toolkit": "^1.9.2",
    "ix": "^5.0.0",
    "nice-grpc": "^2.1.0",
    "rxjs": "^7.8.0",
    "ts-node": "^10.9.1"
  }
}
